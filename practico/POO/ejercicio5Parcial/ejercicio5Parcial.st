Object subclass: #Equipo	instanceVariableNames: 'denominacion categoria personas'	classVariableNames: ''	package: 'ejercicio5Parcial'!!Equipo methodsFor: 'converting' stamp: 'initialze 7/7/2022 17:56'!asString 	^'Equipo categoria: ',categoria asString,' |denominacion: ',denominacion asString.! !!Equipo methodsFor: 'operaciones' stamp: 'initialze 7/8/2022 10:19'!listadoMasValorados: unValor	|listado aux|	aux := ''.	(personas isNotEmpty) ifTrue: [ listado := personas select: [ :x| (x isKindOf: Jugador ) &( x localidad ~='Cordoba') & (x valoracion  > unValor) ] ].	(listado  isNotEmpty) ifTrue: [ listado do: [ :x| aux := aux, x asString, String cr ]].	^ aux ! !!Equipo methodsFor: 'operaciones' stamp: 'initialze 7/7/2022 20:41'!grupoSanguineo 	| cantAB cantB msg |	cantAB := 0.	cantB := 0.	msg := ''.	cantAB := personas select: [ :x | (x isKindOf: Jugador ) & (x grupoSanguineo = 'AB+') ].	cantB := personas select: [ :x | (x isKindOf: Jugador ) & (x grupoSanguineo = 'B-') ] .			((cantB isEmpty ) & (cantAB  isEmpty )) ifTrue:  [ ^ msg := 'que onda' ].		(cantAB size > cantB size)		       ifTrue: [ msg := 'El grupo sanguíneo mas común es AB+' ]		       ifFalse: [ msg := 'El grupo sanguíneo más 	común es B-' ].	^msg.! !!Equipo methodsFor: 'operaciones' stamp: 'initialze 7/8/2022 10:07'!listadoSuspendidos	|listSusp muestra|	muestra := ''.	(personas isNotEmpty )ifTrue: [  listSusp  := personas  select: [ :x| (x isKindOf: Jugador ) & (x estaSuspendido ='Suspendido') & (x posicion ~= 'Arquero') ]].	(listSusp isNotEmpty ) ifTrue: [   listSusp do: [ :x| muestra := muestra ,x asString, String cr ]].	^muestra.! !!Equipo methodsFor: 'operaciones' stamp: 'initialze 7/7/2022 17:55'!agregarPersona: unaPersona	(unaPersona isKindOf: Persona ) ifTrue: [ personas add: unaPersona]! !!Equipo methodsFor: 'operaciones' stamp: 'initialze 7/7/2022 23:01'!jugadoresConFaltas: desde hasta: hasta	|list aux|	aux := ''.	(personas isNotEmpty ) ifTrue: [ list := personas select: [ :x| (x isKindOf: Jugador ) & (x estaSuspendido = 'Suspendido') & (x faltas >=desde)&(x faltas <= hasta) ] ].	(list isNotEmpty ) ifTrue: [ list do: [ :x| aux := aux,x asString , String cr ] ].	^aux! !!Equipo methodsFor: 'operaciones' stamp: 'initialze 7/7/2022 20:41'!agregaJugadoresConPosicion: unaPosicion y: unaValoracion	| listaJugConPosicion aux |	aux:=''.		listaJugConPosicion := personas select: [ :x | (x isKindOf: Jugador) & (x posicion = unaPosicion) & (x valoracion = unaValoracion ) ].	(listaJugConPosicion isNotEmpty ) ifTrue: [ listaJugConPosicion do: [ :x| aux :=aux,x asString, String cr ] ].	^ aux.! !!Equipo methodsFor: 'operaciones' stamp: 'initialze 7/7/2022 19:50'!existeJugador	| datos |	datos := personas detect: [ :x | (x isKindOf: Jugador) & (x posicion = 'Mediocampista') & (x valoracion = 10) & (x lesionado  = false) ] ifNone: [ 'NO ENCONTRADO' ].			^ datos  .! !!Equipo methodsFor: 'initialization' stamp: 'initialze 7/7/2022 17:57'!initialize 	categoria := 0.	denominacion := ''.	personas := OrderedCollection new.! !!Equipo methodsFor: 'accessing' stamp: 'initialze 7/7/2022 17:15'!categoria	^ categoria! !!Equipo methodsFor: 'accessing' stamp: 'initialze 7/7/2022 17:15'!denominacion	^ denominacion! !!Equipo methodsFor: 'accessing' stamp: 'initialze 7/7/2022 17:15'!denominacion: anObject	denominacion := anObject! !!Equipo methodsFor: 'accessing' stamp: 'initialze 7/7/2022 17:15'!categoria: anObject	categoria := anObject! !Object subclass: #Persona	instanceVariableNames: 'nombre apellido localidad grupoSanguineo'	classVariableNames: ''	package: 'ejercicio5Parcial'!!Persona methodsFor: 'initialization' stamp: 'initialze 7/7/2022 20:25'!initialize  	apellido := ''.	grupoSanguineo := ''.	localidad := ''.	nombre := ''.! !!Persona methodsFor: 'converting' stamp: 'initialze 7/7/2022 16:19'!asString 	^'Persona nombre: ',nombre asString ,' |Apellido: ',apellido asString,' |Localidad: ',localidad asString,' |grupoSanguineo: ',grupoSanguineo asString .! !!Persona methodsFor: 'operaciones' stamp: 'initialze 7/7/2022 16:21'!valoracion	^self subclassResponsibility.! !!Persona methodsFor: 'accessing' stamp: 'initialze 7/7/2022 11:10'!nombre	^ nombre! !!Persona methodsFor: 'accessing' stamp: 'initialze 7/7/2022 11:10'!grupoSanguineo	^ grupoSanguineo! !!Persona methodsFor: 'accessing' stamp: 'initialze 7/7/2022 11:10'!localidad: anObject	localidad := anObject! !!Persona methodsFor: 'accessing' stamp: 'initialze 7/7/2022 11:10'!apellido	^ apellido! !!Persona methodsFor: 'accessing' stamp: 'initialze 7/7/2022 11:10'!localidad	^ localidad! !!Persona methodsFor: 'accessing' stamp: 'initialze 7/7/2022 11:10'!apellido: anObject	apellido := anObject! !!Persona methodsFor: 'accessing' stamp: 'initialze 7/7/2022 11:10'!nombre: anObject	nombre := anObject! !!Persona methodsFor: 'accessing' stamp: 'initialze 7/7/2022 11:10'!grupoSanguineo: anObject	grupoSanguineo := anObject! !Persona subclass: #Entrenador	instanceVariableNames: 'cargo antiguedad'	classVariableNames: ''	package: 'ejercicio5Parcial'!!Entrenador methodsFor: 'operaciones' stamp: 'initialze 7/7/2022 16:43'!valoracion 	| valFinal |	valFinal := 0.	(antiguedad between: 1 and: 5)		ifTrue: [ valFinal := 5 ]		ifFalse: [ (antiguedad > 5) ifTrue: [ valFinal := 9 ] ].	(valFinal = 9) ifTrue: [ valFinal := valFinal  + ( (antiguedad - 5) * 2) ].	^ valFinal! !!Entrenador methodsFor: 'accessing' stamp: 'initialze 7/7/2022 16:22'!antiguedad	^ antiguedad! !!Entrenador methodsFor: 'accessing' stamp: 'initialze 7/7/2022 16:22'!antiguedad: anObject	antiguedad := anObject! !!Entrenador methodsFor: 'accessing' stamp: 'initialze 7/7/2022 16:22'!cargo: anObject	cargo := anObject! !!Entrenador methodsFor: 'accessing' stamp: 'initialze 7/7/2022 16:22'!cargo	^ cargo! !!Entrenador methodsFor: 'converting' stamp: 'initialze 7/8/2022 10:15'!asString 	^super asString ,' |cargo: ',cargo asString ,String cr, ' |antiguedad: ',antiguedad asString, ' |valoracion: ', self valoracion asString. ! !!Entrenador methodsFor: 'initialization' stamp: 'initialze 7/7/2022 16:23'!initialize 	super initialize .	"|1 Director tecnico |2 Preparodors fisico |3Ayudante de campo"	cargo := 0.	antiguedad := 0.! !Persona subclass: #Jugador	instanceVariableNames: 'posicion faltas goles lesionado'	classVariableNames: ''	package: 'ejercicio5Parcial'!!Jugador methodsFor: 'accessing' stamp: 'initialze 7/7/2022 16:48'!posicion	^ posicion! !!Jugador methodsFor: 'accessing' stamp: 'initialze 7/7/2022 16:55'!lesionado	^ lesionado! !!Jugador methodsFor: 'accessing' stamp: 'initialze 7/7/2022 16:48'!faltas: anObject	faltas := anObject! !!Jugador methodsFor: 'accessing' stamp: 'initialze 7/7/2022 16:48'!goles: anObject	goles := anObject! !!Jugador methodsFor: 'accessing' stamp: 'initialze 7/7/2022 16:48'!posicion: anObject	posicion := anObject! !!Jugador methodsFor: 'accessing' stamp: 'initialze 7/7/2022 16:55'!lesionado: anObject	lesionado := anObject! !!Jugador methodsFor: 'accessing' stamp: 'initialze 7/7/2022 16:48'!goles	^ goles! !!Jugador methodsFor: 'accessing' stamp: 'initialze 7/7/2022 16:48'!faltas	^ faltas! !!Jugador methodsFor: 'initialization' stamp: 'initialze 7/7/2022 17:52'!initialize 	super initialize .	faltas := 0.	goles := 0.	"1arquero, 2defensa 3central, 4defensa, 5mediocampista, o 6delantero"	posicion :=''.	"0 no  1 si"	lesionado:=false.! !!Jugador methodsFor: 'operaciones' stamp: 'initialze 7/8/2022 10:00'!valoracion	| val |	val := ((lesionado=false) &  "and:" (faltas = 0))		       ifTrue: ["val :=" 10 ]		       ifFalse: [ "val:="5 ].	^ val! !!Jugador methodsFor: 'operaciones' stamp: 'initialze 7/7/2022 17:51'!estaSuspendido	|aux|	aux := 'Activo'.	 (faltas > 3) ifTrue: [ aux:='Suspendido' ].	^aux ! !!Jugador methodsFor: 'converting' stamp: 'initialze 7/7/2022 17:12'!asString 	^super asString ,' |lesionado: ',lesionado asString ,' |faltas: ',faltas asString ,' |Posicion: ',posicion asString , '|goles: ',goles asString ,' |valoracion ', self valoracion asString,' |Activo o Suspendido ', self estaSuspendido asString  . ! !