Object subclass: #Agencia	instanceVariableNames: 'paquetes'	classVariableNames: ''	package: '83013_FranciscoGiardelli'!!Agencia methodsFor: 'accessing' stamp: 'Fran 5/8/2021 16:54'!paquetes	^ paquetes! !!Agencia methodsFor: 'accessing' stamp: 'Fran 5/8/2021 16:54'!paquetes: anObject	paquetes := anObject! !!Agencia methodsFor: 'propios' stamp: 'Fran 5/8/2021 17:42'!mostrarPaqueteServicioBebidas: unTipoServicioBebida	| aux res |	aux := nil.	aux := paquetes detect:[:x | x tipoServicioBebidas = unTipoServicioBebida ].	res:= 'Error de Busqueda.'.	aux isNotNil ifTrue:[res:= aux do:[:x | x nombre asString ]].	^res.! !!Agencia methodsFor: 'propios' stamp: 'Fran 5/8/2021 17:53'!mostrarCrucerosSegunDestino: unDestino	| col res|	col:= paquetes select:[:x | (x isKindOf: Crucero) and: unDestino = x destino].	res:='No hay cruceros para este Destino.'.	col asSortedCollection: [:x :y | x descripcion  > y descripcion].	col isNotEmpty ifTrue:[ res:= 'Lista de Cruceros para el Destino: ', unDestino asString,									col do:[:a | res:=res, String cr, a asString ]].	^res									! !!Agencia methodsFor: 'propios' stamp: 'Fran 5/8/2021 17:49'!addPaquetes:unPaquete	(unPaquete isKindOf:PaqueteTuristico) ifTrue:[ (paquetes detect:[:x | x nombre = unPaquete nombre ]) ifTrue: [paquetes add:unPaquete] ].! !!Agencia methodsFor: 'propios' stamp: 'Fran 5/8/2021 17:21'!costoTotal	| suma |		suma := 0.	paquetes do:[:x | (x duracion > 10) ifTrue:[suma := suma + x costoPaquete] ].	^suma.! !!Agencia methodsFor: 'initialization' stamp: 'Fran 5/8/2021 16:55'!initialize 	paquetes:= OrderedCollection new.! !Object subclass: #AtraccionTuristica	instanceVariableNames: 'nombre costo'	classVariableNames: ''	package: '83013_FranciscoGiardelli'!!AtraccionTuristica methodsFor: 'accessing' stamp: 'Fran 5/8/2021 16:35'!costo	^ costo! !!AtraccionTuristica methodsFor: 'accessing' stamp: 'Fran 5/8/2021 16:35'!nombre: anObject	nombre := anObject! !!AtraccionTuristica methodsFor: 'accessing' stamp: 'Fran 5/8/2021 16:35'!costo: anObject	costo := anObject! !!AtraccionTuristica methodsFor: 'accessing' stamp: 'Fran 5/8/2021 16:35'!nombre	^ nombre! !!AtraccionTuristica methodsFor: 'initialize' stamp: 'Fran 5/8/2021 16:46'!initialize 		nombre := ''.	costo := 0.! !!AtraccionTuristica methodsFor: 'propios' stamp: 'Fran 5/8/2021 16:52'!asString 	^'Nombre: ', nombre asString, String cr,	'Costo: ', costo asString, String cr.	! !Object subclass: #PaqueteTuristico	instanceVariableNames: 'numero descripcion costoBase duracion destino cantidadMillas'	classVariableNames: ''	package: '83013_FranciscoGiardelli'!!PaqueteTuristico methodsFor: 'accessing' stamp: 'Fran 5/8/2021 16:34'!cantidadMillas	^ cantidadMillas! !!PaqueteTuristico methodsFor: 'accessing' stamp: 'Fran 5/8/2021 16:34'!duracion	^ duracion! !!PaqueteTuristico methodsFor: 'accessing' stamp: 'Fran 5/8/2021 16:34'!destino: anObject	destino := anObject! !!PaqueteTuristico methodsFor: 'accessing' stamp: 'Fran 5/8/2021 16:34'!costoBase	^ costoBase! !!PaqueteTuristico methodsFor: 'accessing' stamp: 'Fran 5/8/2021 16:34'!destino	^ destino! !!PaqueteTuristico methodsFor: 'accessing' stamp: 'Fran 5/8/2021 16:34'!descripcion	^ descripcion! !!PaqueteTuristico methodsFor: 'accessing' stamp: 'Fran 5/8/2021 16:34'!cantidadMillas: anObject	cantidadMillas := anObject! !!PaqueteTuristico methodsFor: 'accessing' stamp: 'Fran 5/8/2021 16:34'!costoBase: anObject	costoBase := anObject! !!PaqueteTuristico methodsFor: 'accessing' stamp: 'Fran 5/8/2021 16:34'!duracion: anObject	duracion := anObject! !!PaqueteTuristico methodsFor: 'accessing' stamp: 'Fran 5/8/2021 16:34'!descripcion: anObject	descripcion := anObject! !!PaqueteTuristico methodsFor: 'accessing' stamp: 'Fran 5/8/2021 16:34'!numero	^ numero! !!PaqueteTuristico methodsFor: 'accessing' stamp: 'Fran 5/8/2021 16:34'!numero: anObject	numero := anObject! !!PaqueteTuristico methodsFor: 'initialize' stamp: 'Fran 5/8/2021 16:40'!initialize	numero := 0.	descripcion := ''.	costoBase := 0.	duracion := 0.	destino := ''.	cantidadMillas := 0.! !!PaqueteTuristico methodsFor: 'propios' stamp: 'Fran 5/8/2021 17:03'!costoPaquete	self subclassResponsibility.! !!PaqueteTuristico methodsFor: 'propios' stamp: 'Fran 5/8/2021 16:44'!asString 	^'Número: ',numero asString,String cr,    'Descripción: ',descripcion asString,String cr,    'Costo Base: ', costoBase asString, String cr,	 'Duracion: ', duracion asString, String cr,	 'Destino: ', destino asString, String cr,	 'Cantidad de Millas: ', cantidadMillas asString, String cr.! !PaqueteTuristico subclass: #Catamaran	instanceVariableNames: 'atraccionTuristica tipoServicioBebidas'	classVariableNames: ''	package: '83013_FranciscoGiardelli'!!Catamaran methodsFor: 'propios' stamp: 'Fran 5/8/2021 17:06'!costoPaquete				| suma |		suma := 0.	suma = super costoBase + self atraccionTuristica costo.	^suma.! !!Catamaran methodsFor: 'propios' stamp: 'Fran 5/8/2021 16:48'!asString 		^super asString, String cr,	'Atraccion Turistica: ', atraccionTuristica asString, String cr,	'Tipo de Servicio de Bebidas: ', tipoServicioBebidas asString, String cr.! !!Catamaran methodsFor: 'accessing' stamp: 'Fran 5/8/2021 16:35'!tipoServicioBebidas	^ tipoServicioBebidas! !!Catamaran methodsFor: 'accessing' stamp: 'Fran 5/8/2021 16:35'!tipoServicioBebidas: anObject	tipoServicioBebidas := anObject! !!Catamaran methodsFor: 'accessing' stamp: 'Fran 5/8/2021 16:35'!atraccionTuristica	^ atraccionTuristica! !!Catamaran methodsFor: 'accessing' stamp: 'Fran 5/8/2021 16:35'!atraccionTuristica: anObject	atraccionTuristica := anObject! !!Catamaran methodsFor: 'initialize' stamp: 'Fran 5/8/2021 17:01'!initialize	super initialize .	atraccionTuristica := AtraccionTuristica new.	tipoServicioBebidas := 0.! !PaqueteTuristico subclass: #Crucero	instanceVariableNames: 'nombre cantidadEstrellas'	classVariableNames: ''	package: '83013_FranciscoGiardelli'!!Crucero methodsFor: 'accessing' stamp: 'Fran 5/8/2021 16:35'!nombre: anObject	nombre := anObject! !!Crucero methodsFor: 'accessing' stamp: 'Fran 5/8/2021 16:35'!cantidadEstrellas: anObject	cantidadEstrellas := anObject! !!Crucero methodsFor: 'accessing' stamp: 'Fran 5/8/2021 16:35'!cantidadEstrellas	^ cantidadEstrellas! !!Crucero methodsFor: 'accessing' stamp: 'Fran 5/8/2021 16:35'!nombre	^ nombre! !!Crucero methodsFor: 'propios' stamp: 'Fran 5/8/2021 17:14'!costoPaquete 	| suma |	suma := super costoBase .	(cantidadEstrellas = 4) ifTrue: [ suma += (super costoBase*0.5) ]									ifFalse: [ (cantidadEstrellas = 5) ifTrue: [ suma += super costoBase ] ].	^suma.! !!Crucero methodsFor: 'propios' stamp: 'Fran 5/8/2021 16:49'!asString 	^super asString , String cr,	'Nombre: ', nombre asString, String cr,	'Cantidad de Estrellas: ', cantidadEstrellas asString, String cr.! !!Crucero methodsFor: 'initialize' stamp: 'Fran 5/8/2021 17:01'!initialize		super initialize .	nombre := ''.	cantidadEstrellas := 0.! !